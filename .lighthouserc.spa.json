{
  "ci":{
    "collect":{
      "url": ["/"],
      "staticDistDir":"./dist",
      "isSinglePageApplication": true
    },
    "assert":{
      "assertions":{
        "uses-long-cache-ttl":"off",
        "uses-http2":"off"
      }
    },
    "upload":{
      "target":"filesystem",
      "outputDir":"./dist/_lighthouse",
      "reportFilenamePattern": "%%PATHNAME%%.report.%%EXTENSION%%"
    }
  }
}
